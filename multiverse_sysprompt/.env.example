# OpenRouter Configuration (for Qwen models)
OPENROUTER_API_KEY=
USE_OPENROUTER=true
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1

# OpenAI Configuration (fallback)
OPENAI_API_KEY=your-openai-api-key-here

# Redis Configuration
REDIS_URL=redis://localhost:6379/0

# Model Selection (Qwen via OpenRouter)
PERSONA_MODEL=qwen/qwen-2.5-72b-instruct
CRITIC_MODEL=qwen/qwen-2.5-72b-instruct  
MUTATOR_MODEL=qwen/qwen-2.5-72b-instruct

# Budget & Performance
DAILY_BUDGET_USD=5.0
LAMBDA_TREND=0.3
LAMBDA_SIM=0.2
LAMBDA_DEPTH=0.05

